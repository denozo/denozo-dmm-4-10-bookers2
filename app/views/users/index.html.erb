<h1>ここはusersのページです</h1>

<h1>Users</h1>
    <table>
      <thead>
        <tr>
          <th>image</th>
          <th>name</th>
          <th colspan="3"></th>
        </tr>
      </thead>
      <tbody> <!-- データを入れるテーブル-->
          <% @users.each do |user| %>
        <tr>
          <td><%= attachment_image_tag user, :profile_image, :fill, 60, 60, fallback: "no_image.jpg"%></td>
          <td><%= user.name %></td>
          <td><%= link_to 'Show', user_path(user.id), class: 'show' %></td>
        </tr>
        <% end %>
      </tbody>
    </table>